				<key>ActionName</key>
				<string>ËøêË°åAppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>global DEVICE_NAMEglobal G_TITLEglobal BU_PATHset DEVICE_NAME to "Galaxy Buds (387D)"set BU_PATH to "/usr/local/bin/blueutil" &amp; " "set DEVICE_STATE to getDeviceState()set G_TITLE to "üï∂ 1 Step BT Earphone üéß"try	if DEVICE_STATE then		disconnect()	else		connect()	end if	on error the error_message number the error_number	set the error_text to "Error: " &amp; the error_number &amp; ". " &amp; the error_message	display dialog the error_text buttons {"OK"} default button 1	return the error_textend tryon connect()	set BU_INSTALLED to isBlueUtilInstalled()	if not BU_INSTALLED then		return -- Did not installed Blueutil.	else		setBtPowerOn()		connectDevice()		display notification "‚úÖ Connect " &amp; DEVICE_NAME &amp; " successful!" with title G_TITLE	end ifend connecton disconnect()	set OPTION to " --disconnect \"" &amp; DEVICE_NAME &amp; "\""	display dialog "Disconnect " &amp; DEVICE_NAME &amp; "?" buttons {"Cancel", "‚úÖ Do it now!"} default button 2	if button returned of result = "‚úÖ Do it now!" then		set state to do shell script BU_PATH &amp; OPTION		display notification "‚òëÔ∏è Disconnect " &amp; DEVICE_NAME &amp; " successful!" with title G_TITLE	end ifend disconnecton isBlueUtilInstalled()	set OPTION to " --version"		try		set BU_VERSION to do shell script BU_PATH &amp; OPTION	on error the error_message number the error_number		set the error_text to "‚ùóÔ∏èËØ∑Ê£ÄÊü• Blueutil ÊòØÂê¶ÂÆâË£ÖÔºöhttps://github.com/toy/blueutil " &amp; "Error: " &amp; the error_number &amp; ". " &amp; the error_message		display dialog the error_text buttons {"OK"} default button 1		return the false	end try		return trueend isBlueUtilInstalledon setBtPowerOn()	set OPTION to " --power 1"	return do shell script BU_PATH &amp; OPTIONend setBtPowerOnon connectDevice()	set OPTION to " --connect \"" &amp; DEVICE_NAME &amp; "\""	return do shell script BU_PATH &amp; OPTIONend connectDeviceon getDeviceState()	set OPTION to " --is-connected \"" &amp; DEVICE_NAME &amp; "\""	set state to do shell script BU_PATH &amp; OPTION	if state is equal to "1" then		return true	else		return false	end ifend getDeviceState</string>
